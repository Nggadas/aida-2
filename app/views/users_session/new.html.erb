<%= form_tag authenticate_path do %>

    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>

    <div class="field">
      <%= label_tag :password %>
      <%= text_field_tag :password %>
    </div>


    <div class="actions">
      <%= submit_tag %>
    </div>
<% end %>
